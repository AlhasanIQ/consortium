{
	"name": "Go",
	"build": {
		"dockerfile": "Dockerfile",
		"context": ".."
	},
	"features": {
		"ghcr.io/devcontainers/features/go:1": {
			"version": "latest"
		}
	},
	"forwardPorts": [8080],
	"postCreateCommand": "go mod tidy",
	"customizations": {
		"vscode": {
			"extensions": [
				"golang.Go"
			]
		}
	}
}